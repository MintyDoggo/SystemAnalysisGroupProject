@page "/home"
@inject LoggedInUser loggedInUser


@if (loggedInUser.Login.UserType == UserType.Admin)
{
    <h1>Welcome Admin!</h1>
}
else if (loggedInUser.Login.UserType == UserType.Staff)
{
    <h1>Welcome Staff Member!</h1>
}
else if (loggedInUser.Login.UserType == UserType.Student)
{
    <h1>Welcome Student!</h1>
}

Use the sidebar to navigate to pages
